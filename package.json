{
  "name": "IAmThuyPhamPortfolio",
  "version": "0.1.0",
  "description": "Thuy Pham's portfolio 2018",
  "engines": {
    "node": "6.2.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "web-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "web-dev": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --config webpack.config.js --port $PORT --host $IP --inline --hot --colors",
    "postinstall": "webpack -p"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.2",
    "@fortawesome/fontawesome-free-brands": "^5.0.4",
    "@fortawesome/fontawesome-free-regular": "^5.0.4",
    "@fortawesome/react-fontawesome": "0.0.17",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.8",
    "ejs": "2.5.6",
    "express": "4.15.2",
    "file-loader": "^1.1.6",
    "material-ui": "^1.0.0-beta.28",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-swipeable-views": "^0.12.12",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
